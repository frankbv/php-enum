{
    "name": "frankbv/enum",
    "description": "An enumeration class for PHP",
    "authors": [
        {
            "name": "Frank",
            "homepage": "https://www.frank.shop/"
        }
    ],
    "license": "MIT",
    "type": "library",
    "autoload": {
